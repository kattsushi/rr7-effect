{
  "name": "@devx/web-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "bun run --bun panda codegen",
    "postinstall": "rm -rf node_modules/react-router-dom",
    "dev": "bun run --bun react-router dev",
    "build": "bun run --bun react-router build",
    "start": "bun run --bun react-router-serve ./build/server/index.js",
    "typecheck": "bun run --bun react-router typegen && tsc"
  },
  "dependencies": {
    "@react-router/node": "7.0.0-pre.0",
    "@react-router/serve": "7.0.0-pre.0",
    "effect": "3.8.4",
    "isbot": "^5",
    "match-sorter": "^6.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "7.0.0-pre.0",
    "remix-auth": "^3.7.0",
    "remix-auth-openid": "^0.2.2",
    "sort-by": "^1.2.0",
    "tiny-invariant": "^1.3.3",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.46.1",
    "@react-router/dev": "7.0.0-pre.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
